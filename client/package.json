{
  "name": "poinz-client",
  "version": "0.20.5",
  "description": "Distributed Planning Poker - Credits to Zuehlke/poinz",
  "private": true,
  "author": "ruben@winterrific.net",
  "license": "MIT",
  "homepage": "https://github.com/winterrific/poinz",
  "repository": {
    "type": "git",
    "url": "https://github.com/winterrific/poinz.git"
  },
  "scripts": {
    "start": "npm run serve",
    "lint": "eslint \"app/**/*.js\" \"test/**/*.js\" \"cypress/**/*.js\"",
    "format": "prettier --write \"app/**/*.js\" \"cypress/**/*.js\" \"test/**/*.js\" \"./*.js\"",
    "test": "jest",
    "test:e2e": "npm run cypress:run",
    "pretest": "npm run lint",
    "cypress:run": "cypress run --headed --spec \"cypress/e2e/_all.cy.js\"",
    "cypress:runheadless": "cypress run --headless --spec \"**/!(_all).cy.js\"",
    "cypress:open": "cypress open --e2e",
    "serve": "cross-env BABEL_ENV=development webpack serve --config webpack.config.js --progress",
    "build": "cross-env BABEL_ENV=production webpack --progress --bail --config  webpack.production.config.js",
    "build:analyze": "cross-env BABEL_ENV=production webpack --progress --bail --config  webpack.analysis.config.js"
  },
  "jest": {
    "testMatch": [
      "**/test/unit/**/*Test.js",
      "**/test/integration/**/*Test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!nanoid).+\\.js$"
    ]
  },
  "browserslist": [
    "defaults",
    "edge 18"
  ],
  "dependencies": {
    "history": "5.3.0",
    "loglevel": "1.9.2",
    "nanoid": "5.0.9",
    "purecss": "3.0.0",
    "react": "18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-dropzone": "14.3.5",
    "react-linkify": "^1.0.0-alpha",
    "react-markdown": "^9.0.1",
    "react-redux": "8.1.3",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "remark-gfm": "^4.0.0",
    "reselect": "4.1.8",
    "socket.io-client": "4.8.1",
    "styled-components": "6.1.13",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "1.0.1",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "babel-plugin-styled-components": "2.1.4",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "cypress": "13.17.0",
    "date-fns": "2.30.0",
    "eslint": "^9.17.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-react": "7.37.2",
    "file-loader": "6.2.0",
    "globals": "^15.14.0",
    "jest": "29.7.0",
    "react-test-renderer": "18.2.0",
    "style-loader": "3.3.3",
    "url-loader": "4.1.1",
    "webpack": "5.97.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "engines": {
    "node": ">=20"
  }
}
